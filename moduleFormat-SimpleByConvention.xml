    <Export Artifact="Microsoft.VisualStudio.Services.Zeus.Servicing.dll" Project="Servicing\MS.VS.Services.Zeus.Servicing.csproj" />
    <Export Artifact="Microsoft.VisualStudio.Services.Zeus.Sql.dll" Project="Sql\MS.VS.Services.Zeus.Sql.csproj" />
    <Export Artifact="Microsoft.VisualStudio.Services.Zeus.Web.dll" Project="Web\Zeus\MS.VS.Services.Zeus.Web.csproj" />
